{
  "expo": {
    "name": "Andreev",
    "slug": "athlete",
    "version": "1.1",
    "orientation": "portrait",
    "icon": "./assets/logo.png",
    "userInterfaceStyle": "dark",
    "splash": {
      "image": "./assets/logo.png",
      "resizeMode": "cover",
      "backgroundColor": "#fff"
    },
    "scheme": "Athlete",
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.kuzmin.athlete",
      "buildNumber": "6",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      },
      "jsEngine": "hermes",
      "hermes": {
        "enableHermes": true,
        "enableDebugger": true
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo.png",
        "backgroundColor": "#111"
      },
      "statusBar": {
        "visible": false,
        "hidden": true
      },
      "package": "com.kuzmin.athlete",
      "permissions": [
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.RECORD_AUDIO",
        "android.permission.MODIFY_AUDIO_SETTINGS",
        "android.permission.DETECT_SCREEN_CAPTURE"
      ],
      "jsEngine": "hermes",
      "hermes": {
        "enableHermes": true,
        "enableDebugger": true
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-av",
      "expo-splash-screen"
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "dc346ca7-4762-445a-8d07-9693d277c1b5"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/648d23ed-d8b0-494b-8df0-4420d8d8fdff"
    },
    "owner": "kuzmin"
  }
}
